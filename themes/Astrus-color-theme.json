{
  "name": "Astrus",
  "type": "dark",
  "colors": {
    "editor.background": "#1A2526",
    "editor.foreground": "#E0E0E0",
    "activityBar.background": "#1F2A2B",
    "sideBar.background": "#1F2A2B",
    "statusBar.background": "#2C3E50",
    "statusBar.foreground": "#E0E0E0",
    "editorLineNumber.foreground": "#5C6A72",
    "editor.selectionBackground": "#3B5360",
    "editorCursor.foreground": "#FFD700"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#5C6A72"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#E0E0E0"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#D19A66"
      }
    }
  ]
}