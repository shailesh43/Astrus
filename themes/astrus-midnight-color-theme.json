{
  "name": "Astrus Midnight",
  "type": "dark",
  "colors": {
    "editor.background": "#0D1117",
    "editor.foreground": "#E0E0E0",
    "activityBar.background": "#0D1117",
    "activityBar.foreground": "#42A5F5",
    "sideBar.background": "#0D1117",
    "sideBar.foreground": "#E0E0E0",
    "statusBar.background": "#0D1117",
    "statusBar.foreground": "#42A5F5",
    "editorLineNumber.foreground": "#546E7A",
    "editorLineNumber.activeForeground": "#26A69A",
    "editor.selectionBackground": "#263B5E",
    "editor.selectionHighlightBackground": "#263B5E33",
    "editorCursor.foreground": "#42A5F5",
    "editorHoverWidget.background": "#0D1117",
    "editorHoverWidget.border": "#546E7A",
    "tab.activeBackground": "#0D1117",
    "tab.inactiveBackground": "#0D1117",
    "tab.activeForeground": "#42A5F5",
    "tab.inactiveForeground": "#E0E0E0",
    "panel.background": "#0D1117",
    "panel.border": "#546E7A",
    "terminal.background": "#0D1117",
    "terminal.foreground": "#E0E0E0",
    "titleBar.activeBackground": "#0D1117",
    "titleBar.activeForeground": "#42A5F5",
    "titleBar.inactiveBackground": "#0D1117",
    "titleBar.inactiveForeground": "#E0E0E0"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#546E7A"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#26A69A"
      }
    },
    {
      "scope": ["keyword", "storage", "storage.type"],
      "settings": {
        "foreground": "#42A5F5"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#E0E0E0"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#26A69A"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#CE93D8"
      }
    }
  ]
}